s = input("Enter a string: ")
vowels = consonants = spaces = others = 0
vowels_set = "AEIOUaeiou"

for char in s:
    if char.isalpha():
        if char in vowels_set:
            vowels += 1
        else:
            consonants += 1
    elif char.isspace():
        spaces += 1
    else:
        others += 1

print(f"Vowels: {vowels}, Consonants: {consonants}, Spaces: {spaces}, Others: {others}")


